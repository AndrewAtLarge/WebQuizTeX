  <tr valign="top">
      <td nowrap>&nbsp;<img src="/u/MOW/MathQuiz/Images/right_arrow.gif" alt="" width="9" height="9">&nbsp;</td>
      <td colspan="2" class="headerright"><B>Questions</B></td>
  </tr>
